{
  "environment": {
    "type": "fake-email-inbox-scroll",
    "use_symbol_queries": false,
    "max_steps": 4,
    "num_train": 15,
    "num_test": 6,
    "use_back_action": false,
    "data_dir": "./data_envs_scroll",
    "use_screenshot_cache": true,
    "num_demos": 4000,
    "use_doms": true,
    "use_screenshots": true
  },
  "instruction_agent": {
    "type": "learned",
    "policy": {
      "embedder": {
        "trajectory_embedder": {
          "penalty": 0
        },
        "transition_embedder": {
           "reward_embed_dim": 32
        }
      }
    },
    "update_freq": 4
  },
  "exploration_agent": {
    "policy": {
      "embedder": {
        "experience_embedder": {
          "state_embed_dim": 64,
          "action_embed_dim": 16,
          "embed_dim": 64
        }
      }
    }
  }
}