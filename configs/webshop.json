{
  "environment": {
    "type": "webshop",
    "vlm": "fuyu",
    "quantize": "4b",
    "max_steps": 3,
    "num_train": 100000,
    "num_test": 10000,
    "num_items": 3,
    "window_height": 540,
    "window_width": 960,
    "scroll_amount": 270,
    "scroll_time": 100,
    "num_actions": 5
  },
  "instruction_agent": {
    "type": "learned",
    "policy": {
      "epsilon_schedule": {
        "begin": 1.0,
        "end": 0.05,
        "total_steps": 10000
      },
      "embedder": {
        "trajectory_embedder": {
          "penalty": 0.1,
          "decoder_output_dim": 3
        },
        "transition_embedder": {
          "reward_embed_dim": 32
        }
      },
      "buffer": {
        "sequence_length": 4
      }
    },
    "update_freq": 4
  },
  "exploration_agent": {
    "policy": {
      "epsilon_schedule": {
        "begin": 1.0,
        "end": 0.05,
        "total_steps": 10000
      },
      "embedder": {
        "experience_embedder": {
          "state_embed_dim": 64,
          "action_embed_dim": 16,
          "embed_dim": 64
        }
      }
    },
    "buffer": {
      "sequence_length": 4
    }
  }
}
