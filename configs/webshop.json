{
  "environment": {
    "type": "webshop",
    "max_steps": 3,
    "num_train": 512,
    "num_test": 128,
    "num_items": 3,
    "window_height": 540,
    "window_width": 960,
    "scroll_amount": 270,
    "scroll_time": 100,
    "num_actions": 2,
    "use_screenshot": false,
    "num_demos": 2000
  },
  "instruction_agent": {
    "type": "learned",
    "policy": {
      "epsilon_schedule": {
        "begin": 1.0,
        "end": 0.05,
        "total_steps": 250000
      },
      "embedder": {
        "obs_embedder": {
          "embed_dim": 64
        },
        "trajectory_embedder": {
          "penalty": 0.1,
          "decoder_output_dim": 3
        },
        "transition_embedder": {
          "reward_embed_dim": 32,
          "state_embed_dim": 64
        }
      }
    },
    "buffer": {
      "sequence_length": 4,
      "max_buffer_size": 40000
    },
    "update_freq": 4,
    "min_buffer_size": 500,
    "learning_rate": 0.0001,
    "sync_target_freq": 5000,
    "batch_size": 32,
    "max_grad_norm": 10
  },
  "exploration_agent": {
    "policy": {
      "epsilon_schedule": {
        "begin": 1.0,
        "end": 0.05,
        "total_steps": 80000
      },
      "embedder": {
        "experience_embedder": {
          "state_embed_dim": 64,
          "action_embed_dim": 16,
          "embed_dim": 64
        }
      }
    },
    "buffer": {
      "sequence_length": 4,
      "max_buffer_size": 40000
    },
    "min_buffer_size": 4000,
    "learning_rate": 0.0001,
    "sync_target_freq": 5000,
    "batch_size": 32,
    "update_freq": 4,
    "max_grad_norm": 10
  }
}
